{
  "name": "newport-art-museum-migration",
  "version": "0.0.2",
  "description": "A javascript pipeline for converting the Newport Art Museum's Raiser's Edge Data into a properly formatted set of Salesforce Imports for the new CRM.",
  "main": "main.js",
  "dirs": {
    "source": "./source-data",
    "target": "./target-data"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "individuals": "DATA_TARGET=Individual__c node main.js",
    "organizations": "DATA_TARGET=Organization__c node main.js",
    "households": "DATA_TARGET=Household__c node main.js",
    "gifts": "DATA_TARGET=Gift__c node main.js",
    "memberships": "DATA_TARGET=Membership__c node main.js",
    "cards": "DATA_TARGET=Card__c node main.js",
    "contact1": "DATA_TARGET=Contact1__a node main.js",
    "npsp": "DATA_TARGET=NPSP_Import_Profile__c node main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/work-shop/newport-art-museum-migration.git"
  },
